type Query {
    persons: [Person]
    events: [Event]
}

type Person {
    id: String
    name: String
}

type Book {
    id: ID
    name: String
    pageCount: Int
    author: Author
}

type Author {
    id: ID
    firstName: String
    lastName: String
}

type Event {
    id: Long
    date: String
}

type Subscription {
    counter: Int
}